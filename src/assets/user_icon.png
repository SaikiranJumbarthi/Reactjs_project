		$(".objGrColors").on("move.spectrum change",function(e, tinyColor){
			
				var hexVal = tinyColor.toHexString();
				
				//var sirano=$(this).data( "sira" );
				var siratoplam=$(this).data( "siratoplam" );
				//var renk=$(this).data( "renk" );
				var objid=$(this).data( "objid" );
				var offset_d=$(this).data( "obj_offset" );

				console.log("toplam:"+siratoplam); 
				
				let colorStops = [];
				
				
				for (var i = 0; i < siratoplam; i++) {
					console.log(objects[objid].fill.colorStops[i].offset); 
					if(objects[objid].fill.colorStops[i].offset==offset_d){
						colorStops.push({
							offset	: offset_d,
							color	: hexVal,
							opacity	: 1
						});
						
					}else{
					
					colorStops.push({
							offset	: objects[objid].fill.colorStops[i].offset,
							color	: objects[objid].fill.colorStops[i].color,
							opacity	: 1
						});
					}
				}

				 
				if(objects[objid].fill.type=="linear"){
					var gradient = new fabric.Gradient({
						type:"linear",
						gradientTransform: objects[objid].fill.gradientTransform,
						coords: {
						  x1: objects[objid].fill.coords.x1,
						  y1: objects[objid].fill.coords.y1,
						  x2: objects[objid].fill.coords.x2,
						  y2: objects[objid].fill.coords.y2,
						},
							colorStops : colorStops
					});
				
				}else if(objects[objid].fill.type=="radial"){
			
					var gradient = new fabric.Gradient({
						type:"radial",
						gradientTransform: objects[objid].fill.gradientTransform,
						coords: {
						  r1: objects[objid].fill.coords.r1,
    					  r2: objects[objid].fill.coords.r2,
						  x1: objects[objid].fill.coords.x1,
						  y1: objects[objid].fill.coords.y1,
						  x2: objects[objid].fill.coords.x2,
						  y2: objects[objid].fill.coords.y2,
						
						},
							colorStops : colorStops
					});
					
				}
				 
				 
				 objects[objid].set('fill', gradient);

			canvas.renderAll();
			
			
		});
		
		
		
